{{template "base" .}}

{{define "content"}}
    <div class="row">
        <div class="col">
            <h1 class="text-center mt-4">Choose a room</h1>
            {{$rooms := index .Data "rooms"}}
            {{range $rooms}}
                {{.RoomName}}<br/>
            {{end}}
        </div>
    </div>
{{end}}